<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>
        <record id="wiz_change_product_category_form_view" model="ir.ui.view">
            <field name="name">wiz.change.product.category.form.view</field>
            <field name="model">wiz.change.product.category</field>
            <field name="type">form</field>
            <field name="arch" type="xml">
                <form string="Presences information">
                    <group colspan="4">
                        <field name="category_id" required="1" />
                        <group colspan="2" />
                    </group>
                    <footer>
                        <button name="change_category" type="object"
                            string="Cambiar categoría" class="oe_highlight" />
                        or
                        <button string="Cancelar" class="oe_link" special="cancel"/>
                    </footer>
                </form>
            </field>
        </record>
        <act_window name="Cambiar categoría a produtos"
                    key2="client_action_multi"
                    res_model="wiz.change.product.category"
                    src_model="product.product"
                    view_mode="form"
                    target="new"
                    id="action_wiz_change_product_category"/>
    </data>
</openerp>
