<?xml version="1.0" encoding="UTF-8"?>
<openerp>
<data>
    <record id="quant_search_view_inh_jvv" model="ir.ui.view">
        <field name="name">quant.search.view.inh.jvv</field>
        <field name="model">stock.quant</field>
        <field name="inherit_id" ref="stock.quant_search_view" />
        <field name="arch" type="xml">
            <field name="owner_id" position="after">
				<field name="life_date_without_hour" />
		    </field>
		    <filter string="Company" position="after">
				<filter string="Expiry Date" icon="terp-go-home" domain="[]" context="{'group_by' : 'life_date_without_hour'}"/>
                <filter string="Expiry Year" icon="terp-go-home" domain="[]" context="{'group_by' : 'life_date_without_hour:year'}"/>
                <filter string="Expiry Month" icon="terp-go-home" domain="[]" context="{'group_by' : 'life_date_without_hour:month'}"/>
                <filter string="Expiry Week" icon="terp-go-home" domain="[]" context="{'group_by' : 'life_date_without_hour:week'}"/>
			</filter>
        </field>
    </record>
</data>
</openerp>
