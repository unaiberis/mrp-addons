<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <record id="view_purchase_order_search_inh_jvv_custom" model="ir.ui.view">
        <field name="name">view.purchase.order.search.inh.jvv.custom</field>
        <field name="model">purchase.report</field>
        <field name="inherit_id" ref="purchase.view_purchase_order_search" />
        <field name="arch" type="xml">
            <field name="product_id" position="after">
                <field name="supplier_product_code" />
            </field>
            <filter name="group_partner_id" position="after">
                <filter
            string="Supplier product code"
            name="customer-product-code"
            context="{'group_by':'supplier_product_code'}"
          />
            </filter>
        </field>
    </record>
</odoo>
